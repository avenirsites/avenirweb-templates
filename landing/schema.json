          "defaultValue": "hello@cloudflow.com",
          "required": true
        },
        {
          "key": "footer_phone",
          "label": "Contact Phone",
          "type": "text",
          "defaultValue": "1-800-CLOUD-99",
          "required": false
        },
        {
          "key": "footer_address",

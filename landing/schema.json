    {
      "id": "faq",
      "title": "FAQ Section",
      "description": "Frequently asked questions",
      "fields": [
        {
          "key": "faq_title",
          "label": "FAQ Section Title",
          "type": "text",
          "defaultValue": "Frequently asked questions",
          "required": true
        },
        {
          "key": "faq_subtitle",
          "label": "FAQ Section Subtitle",
          "type": "text",
          "defaultValue": "Everything you need to know about CloudFlow",
          "required": true
        },
        {
          "key": "faq_1_question",
          "label": "FAQ 1 Question",
          "type": "text",
          "defaultValue": "How does the free trial work?",
          "required": true
        },
        {
          "key": "faq_1_answer",
          "label": "FAQ 1 Answer",
          "type": "textarea",
          "defaultValue": "You get 14 days of unlimited access to all features. No credit card required. Cancel anytime.",
          "maxLength": 200,
          "required": true
        },
        {
          "key": "faq_2_question",
          "label": "FAQ 2 Question",
          "type": "text",
          "defaultValue": "Can I change plans later?",
          "required": true
        },
        {
          "key": "faq_2_answer",
          "label": "FAQ 2 Answer",
          "type": "textarea",
          "defaultValue": "Yes! You can upgrade or downgrade your plan at any time. Changes take effect on your next billing cycle.",
          "maxLength": 200,
          "required": true
        },
        {
          "key": "faq_3_question",
          "label": "FAQ 3 Question",
          "type": "text",
          "defaultValue": "Is my data secure?",
          "required": true
        },
        {
          "key": "faq_3_answer",
          "label": "FAQ 3 Answer",
          "type": "textarea",
          "defaultValue": "Absolutely. We use bank-level encryption and are SOC 2 Type II certified. Your data is always safe with us.",
          "maxLength": 200,
          "required": true
        },
        {
          "key": "faq_4_question",
          "label": "FAQ 4 Question",
          "type": "text",
          "defaultValue": "Do you offer custom integrations?",
          "required": true
        },
        {
          "key": "faq_4_answer",
          "label": "FAQ 4 Answer",
          "type": "textarea",
          "defaultValue": "Yes, our Enterprise plan includes custom integrations. We also have a comprehensive API for all plans.",
          "maxLength": 200,
          "required": true
        }
      ]

{
  "id": "landing-saas",
  "name": "SaaS Landing Page",
  "description": "High-converting landing page with pricing tables, features section, and testimonials",
  "category": "business",
  "features": [
    "Hero with video background",
    "Pricing tables",
    "Feature cards",
    "FAQ accordion",
    "Newsletter signup",
    "Social proof section"
  ],
  "customizableElements": {
    "colors": true,
    "fonts": true,
    "layout": true,
    "content": true
  },
  "contentSchema": [
    {
      "id": "metadata",
      "title": "Page Metadata & SEO",
      "description": "Configure page title, description, and SEO settings",
      "fields": [
        {
          "key": "product_name",
          "label": "Product Name",
          "type": "text",
          "defaultValue": "CloudFlow",
          "required": true
        },
        {
          "key": "tagline",
          "label": "Tagline",
          "type": "text",
          "defaultValue": "Modern Solutions for Tomorrow's Challenges",
          "maxLength": 60,
          "required": true
        },
        {
          "key": "tagline_short",
          "label": "Short Tagline",
          "type": "text",
          "defaultValue": "Next-Gen Business Platform",
          "maxLength": 40,
          "required": true
        },
        {
          "key": "meta_description",
          "label": "Meta Description",
          "type": "textarea",
          "defaultValue": "Transform your business with CloudFlow. Boost productivity, streamline operations, and achieve remarkable results with our innovative platform.",
          "maxLength": 160,
          "required": true
        },
        {
          "key": "meta_keywords",
          "label": "Meta Keywords",
          "type": "text",
          "defaultValue": "business software, productivity, cloud platform, SaaS, workflow automation",
          "required": true
        },
        {
          "key": "company_name",
          "label": "Company Name",
          "type": "text",
          "defaultValue": "CloudFlow Inc.",
          "required": true
        },
        {
          "key": "website_url",
          "label": "Website URL",
          "type": "url",
          "defaultValue": "https://www.cloudflow.com",
          "required": true
        },
        {
          "key": "og_description",
          "label": "OG Description",
          "type": "textarea",
          "defaultValue": "CloudFlow - The all-in-one platform that transforms how modern businesses operate. Start your free trial today.",
          "maxLength": 160,
          "required": true
        },
        {
          "key": "og_image_url",
          "label": "OG Image URL",
          "type": "url",
          "defaultValue": "https://www.cloudflow.com/images/og-image.jpg",
          "required": true
        },
        {
          "key": "twitter_description",
          "label": "Twitter Description",
          "type": "textarea",
          "defaultValue": "Transform your business with CloudFlow. Boost productivity and streamline operations.",
          "maxLength": 120,
          "required": true
        },
        {
          "key": "twitter_image_url",
          "label": "Twitter Image URL",
          "type": "url",
          "defaultValue": "https://www.cloudflow.com/images/twitter-card.jpg",
          "required": true
        }
      ]
    },
    {
      "id": "hero",
      "title": "Hero Section",
      "description": "The main headline and call-to-action area",
      "fields": [
        {
          "key": "hero_title",
          "label": "Hero Title",
          "type": "text",
          "defaultValue": "Transform Your Business with Next-Gen Solutions",
          "required": true
        },
        {
          "key": "hero_description",
          "label": "Hero Description",
          "type": "textarea",
          "defaultValue": "Streamline operations, boost productivity, and unlock your team's full potential with CloudFlow - the platform designed for modern businesses.",
          "maxLength": 200,
          "required": true
        },
        {
          "key": "primary_cta",
          "label": "Primary CTA Text",
          "type": "text",
          "defaultValue": "Start Free Trial",
          "required": true
        },
        {
          "key": "secondary_cta",
          "label": "Secondary CTA Text",
          "type": "text",
          "defaultValue": "Watch Demo",
          "required": true
        },
        {
          "key": "trust_text",
          "label": "Trust Badge Text",
          "type": "text",
          "defaultValue": "Join 10,000+ companies already using CloudFlow",
          "required": true
        }
      ]
    },
    {
      "id": "features",
      "title": "Features Section",
      "description": "Highlight your product features",
      "fields": [
        {
          "key": "features_title",
          "label": "Features Section Title",
          "type": "text",
          "defaultValue": "Everything you need to succeed",
          "required": true
        },
        {
          "key": "features_subtitle",
          "label": "Features Section Subtitle",
          "type": "text",
          "defaultValue": "Powerful features designed to help your business thrive",
          "required": true
        },
        {
          "key": "feature_1_title",
          "label": "Feature 1 Title",
          "type": "text",
          "defaultValue": "Real-time Analytics",
          "required": true
        },
        {
          "key": "feature_1_description",
          "label": "Feature 1 Description",
          "type": "textarea",
          "defaultValue": "Get instant insights into your business performance with our advanced analytics dashboard",
          "maxLength": 150,
          "required": true
        },
        {
          "key": "feature_2_title",
          "label": "Feature 2 Title",
          "type": "text",
          "defaultValue": "Team Collaboration",
          "required": true
        },
        {
          "key": "feature_2_description",
          "label": "Feature 2 Description",
          "type": "textarea",
          "defaultValue": "Work seamlessly with your team with built-in chat, file sharing, and project management",
          "maxLength": 150,
          "required": true
        },
        {
          "key": "feature_3_title",
          "label": "Feature 3 Title",
          "type": "text",
          "defaultValue": "Automation Tools",
          "required": true
        },
        {
          "key": "feature_3_description",
          "label": "Feature 3 Description",
          "type": "textarea",
          "defaultValue": "Automate repetitive tasks and workflows to save time and reduce errors",
          "maxLength": 150,
          "required": true
        },
        {
          "key": "feature_4_title",
          "label": "Feature 4 Title",
          "type": "text",
          "defaultValue": "Enterprise Security",
          "required": true
        },
        {
          "key": "feature_4_description",
          "label": "Feature 4 Description",
          "type": "textarea",
          "defaultValue": "Bank-level encryption and security measures to keep your data safe and compliant",
          "maxLength": 150,
          "required": true
        },
        {
          "key": "feature_5_title",
          "label": "Feature 5 Title",
          "type": "text",
          "defaultValue": "24/7 Support",
          "required": true
        },
        {
          "key": "feature_5_description",
          "label": "Feature 5 Description",
          "type": "textarea",
          "defaultValue": "Get help whenever you need it with our dedicated support team available round the clock",
          "maxLength": 150,
          "required": true
        },
        {
          "key": "feature_6_title",
          "label": "Feature 6 Title",
          "type": "text",
          "defaultValue": "API Integration",
          "required": true
        },
        {
          "key": "feature_6_description",
          "label": "Feature 6 Description",
          "type": "textarea",
          "defaultValue": "Connect with your favorite tools and services through our comprehensive API",
          "maxLength": 150,
          "required": true
        }
      ]
    },
    {
      "id": "pricing",
